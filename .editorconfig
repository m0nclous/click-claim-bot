# http://editorconfig.org

[*]
charset = UTF-8
indent_style = space
indent_size = 4
end_of_line = LF
insert_final_newline = true
trim_trailing_whitespace = true
max_line_length = 110

[*.json]
insert_final_newline = unset

[**.min.js]
indent_style = unset
insert_final_newline = unset

[MakeFile]
indent_style = space

[*.md]
trim_trailing_whitespace = false
